#include <bits/stdc++.h>
#define ll long long int
using namespace std;

struct FenwickTree {
    vector<ll> bit;
    int MaxVal;
    FenwickTree(int n) {
        MaxVal = n;
        bit.assign(n, 0);
    }
    ll prefix_sum(int r) {
        ll ret = 0;
        while ( r >= 0 )
        {
            ret += bit[r];
            r = (r & (r + 1)) - 1;
        }
        return ret;
    }
    ll range_sum(int l, int r) {
        l--, r--;
        if(l == 0) 
            return prefix_sum(r);
        return prefix_sum(r) - prefix_sum(l - 1);
    }
    void single_update(int idx, ll val) {
        while (idx < MaxVal) {
            bit[idx] += val;
            idx |= (idx + 1);
        }
    }
    void range_update(int l, int r, ll val)
    {
        l--, r--;
        single_update(l, val);
        single_update(r + 1, -val);
    }
};
int main() {
    int n, q;
    cin >> n >> q;
    FenwickTree bit(n);
    for (int i = 0; i < n; ++i) {
        ll val;
        cin >> val;
        bit.single_update(i, val); 
    }
    for(int i = 0; i < n; i++)
        cout<<bit.prefix_sum(i)<<' ';
    cout<<endl;
    cout<<bit.range_sum(2, 4)<<endl;
}
